<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="GET запрос /form">
    <test name="Проверка всего запроса form">
        <groups>
            <run>
                <include name="form" />
            </run>
        </groups>
        <classes>
            <class name="tests.positive.StatusCode" />
            <class name="tests.positive.ValidateHeaders"/>
            <class name="tests.positive.ValidateResponse"/>
            <class name="tests.negative.validData.StatusCode"/>
            <class name="tests.negative.validData.CorrectLoading"/>
            <class name="tests.negative.invalidData.StatusCode"/>
            <class name="tests.negative.invalidData.CorrectLoading"/>
        </classes>
    </test>
</suite>